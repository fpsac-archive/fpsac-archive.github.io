% Einstellen von grundlegenden Parametern etc.:                            %
%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%
\unitlength = 5mm             % In dieser Einheit wird skaliert
 
\thicklines                     %Dicke Linien
 
\newcount\xmin                % Koordinaten der extremalen Punkte
\newcount\xmax
\newcount\ymin
\newcount\ymax
 
\newcount\gridwidth           % Breite und Hoehe des Gitters
\newcount\gridheight
 
\newcount\nofxpoints          % Anzahl der Punkte in waagrechter Richtung
\newcount\nofypoints          % Anzahl der Punkte in senkrechter Richtung
 
\newcount\dummycounta         % Hilfsvariable
\newcount\dummycountb
 
\def\begingrid#1#2#3#4{
	% \begingroup Wozu denn?
	\global\xmin = #1          % Belege die extremalen Gitterpunkte
	\global\ymin = #2
	\global\xmax = #3
	\global\ymax = #4
	
	% Sind die Angaben auch sinnvoll?
	\ifnum\xmin > \xmax
		\errmessage{GRIDS: \xmin > \xmax|}
	\fi
	\ifnum\ymin > \ymax
		\errmessage{GRIDS: \ymin > \ymax|}
	\fi
	% Berechne Hoehe, Breite und die Anzahl der Punkte in x- und y-Richtung
	\gridwidth = \xmax
	\gridheight = \ymax
	\advance\gridwidth by -\xmin
	\advance\gridheight by -\ymin
	\nofxpoints = \gridwidth
	\advance\nofxpoints by 1
	\nofypoints = \gridheight
	\advance\nofypoints by 1
	
	% Wir wollen ein bisschen Platz rund um unsere Graphik:
	%\advance\gridwidth by 4
	%\advance\gridheight by 4
	%\advance\xmin by -2
	%\advance\ymin by -2
	\begin{picture}(\gridwidth, \gridheight)(\xmin, \ymin)
	\message{Start of LaTeX-picture: }
	% Im Folgenden sollen aber die Zahlen wieder stimmen:
	%\advance\gridwidth by -4
	%\advance\gridheight by -4
	%\advance\xmin by 2
	%\advance\ymin by 2
	\message{GRID: width \number\gridwidth, height \number\gridheight; %
				at (\number\xmin, \number\ymin)}
	% Wir zeichnen mit einer doppelten Schleife das Gitter:
	\global\dummycounta = \ymin
	\loop\ifnum\dummycounta<\ymax
		\makegridline\dummycounta
		\global\advance\dummycounta by 1
	\repeat
	\makegridline\ymax
}
 
% Die innere Schleife muss man offenbar so auslagern:
\def\makegridline#1{
		\begingroup
		\global\dummycountb = \xmin
		\loop\ifnum\dummycountb<\xmax
			\put(\dummycountb, #1){\circle*{0.1}}
			\global\advance\dummycountb by 1
		\repeat
		\put(\xmax, #1){\circle*{0.1}}
		\endgroup
		}
 
% Achsensystem zeichnen:
\def\makexaxes{\thinlines
	\dummycounta = \gridwidth
	\advance\dummycounta by 1
	\put(\xmin, 0){\vector(1,0){\dummycounta}}
        \thicklines
}
\def\makeyaxes{\thinlines
	\dummycountb = \gridheight
	\advance\dummycountb by 1
	\put(0, \ymin){\vector(0,1){\dummycountb}}
        \thicklines
}
\def\makeaxes{\thinlines
	\dummycounta = \gridwidth
	\advance\dummycounta by 1
	\put(\xmin, 0){\vector(1,0){\dummycounta}}
	\dummycountb = \gridheight
	\advance\dummycountb by 1
	\put(0, \ymin){\vector(0,1){\dummycountb}}
        \thicklines
}
 
% X-Achse skalieren:
\def\makexscale{
	\dummycounta = \xmin
	\loop\ifnum\dummycounta<0
		\put(\dummycounta, -0.4){\makebox(0,0){{\scalefont\number\dummycounta}}}
		\advance\dummycounta by 1
	\repeat
	\dummycounta = 1
	\loop\ifnum\dummycounta<\xmax
		\put(\dummycounta, -0.4){\makebox(0,0){{\scalefont\number\dummycounta}}}
		\advance\dummycounta by 1
	\repeat
	\put(\xmax, -0.4){\makebox(0,0){{\scalefont\number\xmax}}}
}
 
% Y-Achse skalieren:
\def\makeyscale{
	\dummycounta = \ymin
	\loop\ifnum\dummycounta<0
		\put(-0.4, \dummycounta){\makebox(0,0){{\scalefont\number\dummycounta}}}
		\advance\dummycounta by 1
	\repeat
	\dummycounta = 1
	\loop\ifnum\dummycounta<\ymax
		\put(-0.4, \dummycounta){\makebox(0,0){{\scalefont\number\dummycounta}}}
		\advance\dummycounta by 1
	\repeat
	\put(-0.4, \ymax){\makebox(0,0){{\scalefont\number\ymax}}}
}
 
% Dicken Punkt ins Koordinatensystem zeichnen:
