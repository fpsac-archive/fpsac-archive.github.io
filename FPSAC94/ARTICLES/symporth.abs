	\ifnum#2<\ymin\errmessage{GRIDS: gridpoint beneath picture}\fi
	\ifnum#2>\ymax\errmessage{GRIDS: gridpoint above picture}\fi
	\put(#1, #2){\circle*{0.2}}
}
 
